<h1>Search User fitbit data</h1>

<%= form_tag(generate_report_user_path method: :get) do %>
    Username: <%= text_field_tag("search_range[user_name]", @display_user_name) %> <br/>
    Start Date <%= date_field :search_range, :start_date, :value => @display_start_date %> <br/>
    End Date <%= date_field :search_range, :end_date, :value => @display_end_date %> <br/>
    <%= submit_tag 'Search' %>
<% end %>

<% if @fitbit_data_list.present? %>
<%= render 'list_fitbit_data' %>
<% end %>